{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "ESNext"
    ],
    "module": "nodenext",
    "baseUrl": ".",
    "types": [
      "node"
    ],
    "resolveJsonModule": true,
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "noEmitOnError": true,
    "declarationDir": "./lib",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
